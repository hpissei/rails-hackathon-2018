<p id="notice"><%= notice %></p>

<h1>Person2s</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Date of birth</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @person2s.each do |person2| %>
      <tr>
        <td><%= person2.first_name %></td>
        <td><%= person2.last_name %></td>
        <td><%= person2.date_of_birth %></td>
        <td><%= person2.age %></td>
        <td><%= link_to 'Show', person2 %></td>
        <td><%= link_to 'Edit', edit_person2_path(person2) %></td>
        <td><%= link_to 'Destroy', person2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person2', new_person2_path %>
